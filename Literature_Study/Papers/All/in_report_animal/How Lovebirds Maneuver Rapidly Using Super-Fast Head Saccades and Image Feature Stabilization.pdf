data-annotation-id="10R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -306.595px -394.413px 0px; left: 306.595px; top: 394.413px; width: 4.819px; height: 10.034px;" class="linkAnnotation"><a href="#%FE%FF%00R%00p%00o%00n%00e%00.%000%001%002%009%002%008%007%00.%00r%00e%00f%000%000%009" class="internalLink"></a></section><section data-annotation-id="11R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -307.616px -407.395px 0px; left: 307.616px; top: 407.395px; width: 9.637px; height: 10.035px;" class="linkAnnotation"><a href="#%FE%FF%00R%00p%00o%00n%00e%00.%000%001%002%009%002%008%007%00.%00r%00e%00f%000%001%000" class="internalLink"></a></section><section data-annotation-id="12R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -492.491px -472.422px 0px; left: 492.491px; top: 472.422px; width: 9.581px; height: 9.978px;" class="linkAnnotation"><a href="#%FE%FF%00R%00p%00o%00n%00e%00.%000%001%002%009%002%008%007%00.%00r%00e%00f%000%001%001" class="internalLink"></a></section><section data-annotation-id="13R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -504.283px -472.422px 0px; left: 504.283px; top: 472.422px; width: 9.581px; height: 9.978px;" class="linkAnnotation"><a href="#%FE%FF%00R%00p%00o%00n%00e%00.%000%001%002%009%002%008%007%00.%00r%00e%00f%000%001%002" class="internalLink"></a></section><section data-annotation-id="14R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -305.348px -602.419px 0px; left: 305.348px; top: 602.419px; width: 9.581px; height: 9.978px;" class="linkAnnotation"><a href="#%FE%FF%00R%00p%00o%00n%00e%00.%000%001%002%009%002%008%007%00.%00r%00e%00f%000%001%003" class="internalLink"></a></section><section data-annotation-id="15R" style="transform: matrix(1.39507, 0, 0, 1.39507, 0, 0); transform-origin: -317.14px -602.419px 0px; left: 317.14px; top: 602.419px; width: 9.581px; height: 9.978px;" class="linkAnnota